  width: 100%
  background-position: 50% 0%
  min-height: 176px
  min-width: 431px
  background-image: url("images/feedback_world.png")
  background-size: contain
  background-repeat: no-repeat

.home-instruction
  @extend %flex-c
  @extend %t-small
  flex-direction: column
  text-align: center